{
   "name": "harm-syn",
   "version": "1.0.1",
   "description": "An analysis and synthesis algorithm for quasi-periodic signals, e.g. vowels.",
   "type": "module",
   "types": "dist/Index.d.ts",
   "main": "dist/Index.js",
   "exports": {
      ".": {
         "types": "./dist/Index.d.ts",
         "default": "./dist/Index.js"
      },
      "./*": {
         "types": "./dist/*.d.ts",
         "default": "./dist/*.js"
      }
   },
   "keywords": [
      "harmsyn",
      "harmonic-analysis",
      "harmonic-synthesis",
      "sound-analysis",
      "sound-synthesis",
      "sound",
      "frequency-tracking",
      "instantaneous-frequency"
   ],
   "license": "MIT",
   "homepage": "http://www.source-code.biz/harmSyn",
   "repository": {
      "type": "git",
      "url": "git+https://github.com/chdh/harm-syn.git"
   },
   "contributors": [
      {
         "name": "Christian d'Heureuse",
         "email": "chdh@inventec.ch",
         "url": "http://www.source-code.biz"
      }
   ],
   "scripts": {
      "build": "tsc",
      "lint": "eslint src/**"
   },
   "dependencies": {
      "commons-math-interpolation": "^2.2.4",
      "dsp-collection": "^0.2.8"
   },
   "files": [
      "dist/"
   ]
}
